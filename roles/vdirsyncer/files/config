[general]
status_path = "~/.cache/vdirsyncer/status/"

[storage local_calendars]
type = "filesystem"
path = "~/.calendars"
fileext = ".ics"

[storage etesync_calendars]
type = "etesync_calendars"
email = "<< pass Passwords/Internet/EteSync/Email >>"
secrets_dir = "/home/dan/.cache/vdirsyncer/secrets"

[pair calendars]
a = "etesync_calendars"
b = "local_calendars"
collections = << pass ShellSecrets/vdirsyncer/collections_calendars >>

[storage local_contacts]
type = "filesystem"
path = "~/.contacts"
fileext = ".vcf"

[storage etesync_contacts]
type = "etesync_contacts"
email = "<< pass Passwords/Internet/EteSync/Email >>"
secrets_dir = "/home/dan/.cache/vdirsyncer/secrets"

[pair contacts]
a = "etesync_contacts"
b = "local_contacts"
collections = << pass ShellSecrets/vdirsyncer/collections_contacts >>
