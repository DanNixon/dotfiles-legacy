exec --no-startup-id xrandr --output DVI-I-1 --rotate left --left-of DVI-D-1 --auto

## My Model M doesn't have a Super_L key :(
set $mod Mod1

## Workspace moving (custom desktop PC)
bindsym $mod+Shift+greater move workspace to output DVI-D-1
bindsym $mod+Shift+less move workspace to output DVI-I-1
