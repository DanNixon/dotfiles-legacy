set last_window_maximized True
set last_window_width 985
set last_window_height 505

set port /dev/ttyACM0
set baudrate 115200

set uimode Standard
set tempgauges True
set lockbox True
set monitor True

button 0 "EStop" /c "#FF0000" M112
button 1 "GetPosition" /c "#77EEFF" M114
button 4 "SetZ0" /c "#88CC22" G92 Z0
button 5 "MovePrintHeight" /c "#FF9966" G1 Z0.1 F75
button 6 "MoveZ10" /c "#FF9966" G1 Z10 F100
